extends base.pug
block scripts 
  script(src='/static/index.js')
block style 
    style
        include ../static/style.css
        include ../static/stylecontact.css

block content 
    div.container
        h1 Contact Us
        form(action="/contact" ,class="myform",method="post")
            input(type="text" class="myinput",name="name",placeholder="Enter Your Name") 
            input(type="phone" class="myinput",name="phone",placeholder="Enter Your Mobile no.") 
            input(type="email" class="myinput",name="email",placeholder="Enter Your Email id") 
            input(type="text" class="myinput",name="address",placeholder="Enter Your Address") 
            input(type="text" class="myinput",name="concern",placeholder="Enter Your Concern") 
            button(class="btn")
             | submit


